{
  "expo": {
    "name": "flota_app",
    "slug": "flota_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1e3a8a"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1e3a8a"
      },
      "package": "cl.clagria.flota",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDpJ18HhslupA_BcCaeZW5vQiFsVIf1leU"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "La app necesita acceder a tus fotos para subir evidencia.",
          "cameraPermission": "La app necesita acceder a la cámara para tomar fotos del odómetro."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permitir a Flota Somyl rastrear la ruta durante el viaje."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "1b3fd2aa-3ab6-4132-850a-ac26536ec212"
      }
    }
  }
}
